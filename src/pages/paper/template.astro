---
import Container from "../../components/Container.astro";
import Heading2 from "../../components/Heading2.astro";
import Layout2 from "../../layouts/Layout2.astro";
import ButtonContainer from "../../components/ButtonContainer.astro";
import Button from "../../components/Button.astro";
import Heading3 from "../../components/Heading3.astro";

---

<Layout2>
    <Heading2 
        text="Paper Template"
    />
    <Container>
        <ButtonContainer>
            <Heading3
                text="International"
            />
            <Button
                href="https://drive.google.com/uc?export=download&id=1F29PixCxvvVMV1-wvsn9pdh8ntJ3MT-C"
                class="fa-solid fa-download"
                title="Abstract Template"
                description="Click this button to download Abstract Template."
            />
        </ButtonContainer>
        <ButtonContainer>
            <Button
                href="https://drive.google.com/uc?export=download&id=1XJPSratas5NUiJjqdDst0C441QH1jHVZ"
                class="fa-solid fa-download"
                title="Full Paper Template"
                description="Click this button to download Full Paper Template."
            />
        </ButtonContainer>
        <ButtonContainer>
            <Heading3
                text="National"
            />
            <Button
                href="https://drive.google.com/uc?export=download&id=1dVOGJ8wriRZFRpNodTjI-y5uhAY81Q32"
                class="fa-solid fa-download"
                title="Abstract Template"
                description="Click this button to download Abstract Template."
            />
        </ButtonContainer>
        <ButtonContainer>
            <Button
                href="https://drive.google.com/uc?export=download&id=1Bfyo2FBobkycBCslRj9fBnu3I0q2gx_R"
                class="fa-solid fa-download"
                title="Full Paper Template"
                description="Click this button to download Full Paper Template."
            />
        </ButtonContainer>
    </Container>
</Layout2>